    "libft.h": "c",
    "stdbool.h": "c",
    "cstdio": "c",
    "*.tcc": "c",
    "compare": "c",
    "random": "c",
    "limits": "c",
    "new": "c",
    "numeric": "c",
    "cmath": "c",
    "tuple": "c",
    "type_traits": "c",
    "utility": "c",
    "complex": "c",
    "ft_libbmp.h": "c"
  }
}
